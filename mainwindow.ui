   <string notr="true">/* =========================
   BASE APP
========================= */
QWidget{
    font-family: &quot;Segoe UI&quot;, &quot;Poppins&quot;;
    font-size: 10.5pt;
    color: #111827;
    background: transparent;
}

QMainWindow{
    background-color: #F7F7FB;
}

/* =========================
   &quot;CARDS&quot; / PANELS
========================= */
QFrame#card, QWidget#card{
    background-color: rgba(255,255,255,210);
    border: 1px solid rgba(17,24,39,35);
    border-radius: 16px;
}

/* =========================
   SIDEBAR
========================= */
QFrame#sidebar{
    background-color: rgba(255,255,255,200);
    border-right: 1px solid rgba(17,24,39,30);
    border-radius: 18px;
}

/* Boutons menu */
QPushButton#navBtn{
    background: rgba(255,255,255,160);
    border: 1px solid rgba(17,24,39,28);
    border-radius: 12px;
    padding: 10px 14px;
    text-align: left;
    color: #111827;
}

QPushButton#navBtn:hover{
    background: rgba(0,206,209,35);
    border: 1px solid rgba(0,206,209,120);
}

QPushButton#navBtn:checked{
    background: rgba(0,206,209,55);
    border: 1px solid rgba(0,206,209,170);
    color: #0F172A;
    font-weight: 600;
}

/* =========================
   TITLES / LABELS
========================= */
QLabel#titleLabel{
    background-color: rgba(255,255,255,170);
    border: 1px solid rgba(17,24,39,30);
    border-radius: 18px;
    padding: 10px 22px;

    font-size: 18pt;
    font-weight: 700;
    letter-spacing: 1px;
    color: #0F172A;
}

QLabel#mutedLabel{
    color: #6B7280;
}

/* =========================
   INPUTS
========================= */
QLineEdit, QPlainTextEdit, QTextEdit, QSpinBox, QDoubleSpinBox, QDateEdit, QComboBox{
    background-color: rgba(255,255,255,220);
    border: 1px solid rgba(17,24,39,30);
    border-radius: 12px;
    padding: 8px 12px;
    selection-background-color: rgba(0,206,209,70);
}

QLineEdit:focus, QPlainTextEdit:focus, QTextEdit:focus, QComboBox:focus{
    border: 1px solid rgba(0,206,209,180);
}

/* ComboBox */
QComboBox::drop-down{
    border: none;
    width: 30px;
}
QComboBox::down-arrow{
    width: 10px; height: 10px;
}

/* =========================
   BUTTONS (Modern)
========================= */
QPushButton{
    background: rgba(255,255,255,220);
    border: 1px solid rgba(17,24,39,35);
    border-radius: 12px;
    padding: 9px 16px;
    color: #111827;
}

QPushButton:hover{
    background: rgba(0,206,209,35);
    border: 1px solid rgba(0,206,209,140);
}

QPushButton:pressed{
    background: rgba(0,206,209,55);
}

/* Primary button */
QPushButton#primaryBtn{
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #7FFFD4, stop:1 #00CED1);
    border: none;
    color: #0B1220;
    font-weight: 700;
}
QPushButton#primaryBtn:hover{
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #61F0D9, stop:1 #00B8BD);
    color: white;
}
QPushButton#primaryBtn:pressed{
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #41D8C4, stop:1 #009095);
}

/* Danger button (supprimer) */
QPushButton#dangerBtn{
    background: #E53935;
    border: none;
    color: white;
    font-weight: 700;
}
QPushButton#dangerBtn:hover{ background: #D32F2F; }
QPushButton#dangerBtn:pressed{ background: #B71C1C; }

/* Disabled */
QPushButton:disabled{
    background: #E5E7EB;
    color: #9CA3AF;
    border: 1px solid #D1D5DB;
}

/* =========================
   GROUPBOX (glass)
========================= */
QGroupBox{
    background-color: rgba(255,255,255,85);
    border: 1px solid rgba(255,255,255,140);
    border-radius: 18px;
    margin-top: 18px;
    padding: 14px;
}

QGroupBox::title{
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 4px 12px;
    margin-left: 14px;
    background-color: rgba(255,255,255,120);
    border: 1px solid rgba(255,255,255,150);
    border-radius: 10px;
    font-weight: 700;
    color: #0F172A;
}

/* =========================
   TABLE / TREE
========================= */
QTableView, QTreeView{
    background: rgba(255,255,255,210);
    border: 1px solid rgba(17,24,39,30);
    border-radius: 14px;
    gridline-color: rgba(17,24,39,18);
    selection-background-color: rgba(0,206,209,55);
    selection-color: #0B1220;
}

QHeaderView::section{
    background-color: rgba(255,255,255,230);
    border: none;
    border-bottom: 1px solid rgba(17,24,39,25);
    padding: 8px 10px;
    font-weight: 700;
    color: #0F172A;
}

QTableView::item{
    padding: 6px;
    border-radius: 8px;
}

QTableView::item:hover{
    background: rgba(0,206,209,25);
}

/* =========================
   SCROLLBAR (clean)
========================= */
QScrollBar:vertical{
    background: transparent;
    width: 10px;
    margin: 8px 4px 8px 4px;
}
QScrollBar::handle:vertical{
    background: rgba(17,24,39,60);
    border-radius: 5px;
    min-height: 30px;
}
QScrollBar::handle:vertical:hover{
    background: rgba(0,206,209,180);
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical{
    height: 0px;
    background: transparent;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical{
    background: transparent;
}
</string>
